  <data key="d4">lebo105.pdf</data>
  <data key="d5">1762239978</data>
</node>
<node id="Monozygotic Twins">
  <data key="d0">Monozygotic Twins</data>
  <data key="d1">category</data>
  <data key="d2">Monozygotic twins are genetically identical individuals who share the same DNA fingerprint pattern.</data>
