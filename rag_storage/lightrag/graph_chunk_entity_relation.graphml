  <data key="d4">lebo105.pdf</data>
  <data key="d5">1762239978</data>
</node>
<node id="Nucleotides">
  <data key="d0">Nucleotides</data>
  <data key="d1">category</data>
  <data key="d2">Nucleotides are the building blocks of nucleic acids, forming long polymers such as DNA and RNA.</data>
